{
    "expo": {
        "name": "Eshal",
        "slug": "Eshal",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "eshal",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.expotesttool.Eshal"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-video",
            [
                "expo-build-properties",
                {
                    "ios": {
                        "newArchEnabled": true,
                        "extraPods": [
                            {
                                "name": "simdjson",
                                "configurations": ["Debug", "Release"],
                                "path": "../node_modules/@nozbe/simdjson",
                                "modular_headers": true
                            }
                        ]
                    },
                    "android": {
                        "newArchEnabled": true,
                        "kotlinVersion": "1.6.10",
                        "packagingOptions": {
                            "pickFirst": ["**/libc++_shared.so"]
                        }
                    }
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        "./src/assets/fonts/Inter/static/Inter-Black.ttf",
                        "./src/assets/fonts/Inter/static/Inter-Bold.ttf",
                        "./src/assets/fonts/Inter/static/Inter-ExtraBold.ttf",
                        "./src/assets/fonts/Inter/static/Inter-ExtraLight.ttf",
                        "./src/assets/fonts/Inter/static/Inter-Light.ttf",
                        "./src/assets/fonts/Inter/static/Inter-Medium.ttf",
                        "./src/assets/fonts/Inter/static/Inter-Regular.ttf",
                        "./src/assets/fonts/Inter/static/Inter-SemiBold.ttf",
                        "./src/assets/fonts/Inter/static/Inter-Thin.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-Bold.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-BoldItalic.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-ExtraLight.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-ExtraLightItalic.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-Italic.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-Light.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-LightItalic.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-Medium.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-MediumItalic.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-Regular.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-SemiBold.ttf",
                        "./src/assets/fonts/Kodchasan/Kodchasan-SemiBoldItalic.ttf"
                    ]
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
